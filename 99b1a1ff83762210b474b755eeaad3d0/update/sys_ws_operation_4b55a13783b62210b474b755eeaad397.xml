        <http_method>GET</http_method>
        <name>Fetch Priorities</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var result = [];
    var gr = new GlideRecord('sys_choice');
    gr.addQuery('name', 'task');      // Targeting the 'incident' table
    gr.addQuery('element', 'priority');   // Targeting the 'priority' field
    gr.query();

    while (gr.next()) {
        result.push({
            sys_id: gr.sys_id.toString(),
            label: gr.label.toString(),
            value: gr.value.toString()
        });
    }

    response.setStatus(200);
    response.setBody({
        success: true,
        total: result.length,
        priorities: result
    });

})(request, response);
]]></operation_script>
